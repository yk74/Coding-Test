from collections import defaultdict

def solution(tickets):
    answer = []
    lst = defaultdict(list)

    for i in tickets:
        lst[i[0]].append(i[1])

    for key in lst.keys():
        lst[key].sort(reverse=True)

    stack = ['ICN']
    while stack:
        tmp = stack[-1]

        if not lst[tmp]:
            answer.append(stack.pop())
        else:
            stack.append(lst[tmp].pop())
    answer.reverse()


    return answer
